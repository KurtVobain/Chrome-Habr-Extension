{
   "manifest_version": 3,
   "name": "Hide Habr sidebar",
   "version": "1.3.0",
   "description": "Delete annoying sidebar with recommended articles from Habr",
   "icons": {
       "128": "128.png"
   },
   "content_scripts": [
       {
           "matches": [ "https://habr.com/*" ],
           "js": [ "app.js" ]
       }
   ]
}